	<script>
		window.onLoad = (function() {

                             for (let i = 0; i < 4; i++) {

                  var fileName = "/nieco" + i + ".pdf"
                    MobileCRM.Application.deleteFile(fileName, function(res) {
                        MobileCRM.bridge.alert("File was deleted");
                    }, function(err) {
                        MobileCRM.bridge.alert("File was not deleted");
                    }, null)
                    }
 })();

	</script>
